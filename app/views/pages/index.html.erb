<div class="landing-page">

  <div class="container landing-page-contents">
    <h1>Use My Kitchen</h1>
    <h3>Find the kitchen and dining space to suit your next dinner party.</h3>

    <div class="row margin search-bar">
      <form action="/kitchens">
        <div class="row">
          <div class="col s3 offset-s1">
            <input id="location" name="location" type="text" placeholder="Location">
          </div>
          <div class="col s3">
            <input id="date" name="date" type="date" placeholder="Date">
          </div>
          <div class="col s2">
            <input id="guests" name="guests" type="number" placeholder="Guests">
          </div>
          <div class="col s1">
            <button class="waves-effect waves-light btn-large">Search</button>
          </div>
        </div>
      </form>
    </div>

  </div>

</div>
